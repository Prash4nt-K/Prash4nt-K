<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ESeva</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  
  <servlet>
  	<servlet-name>pair1</servlet-name>
  	<servlet-class>ESeva.Servlet.Login</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>pair1</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>pair2</servlet-name>
  	<servlet-class>ESeva.Servlet.RegisterViolation</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>pair2</servlet-name>
  	<url-pattern>/registerViolation</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>pair3</servlet-name>
  	<jsp-file>/ShowIssued.jsp</jsp-file>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>pair3</servlet-name>
  	<url-pattern>/redi</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>pair4</servlet-name>
  	<jsp-file>/ShowPayment.jsp</jsp-file>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>pair4</servlet-name>
  	<url-pattern>/RediPay</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>pair5</servlet-name>
  	<servlet-class>ESeva.Servlet.RegisterEmergency</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>pair5</servlet-name>
  	<url-pattern>/regEm</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>pair6</servlet-name>
  	<servlet-class>ESeva.Servlet.SubmitPaymentData</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>pair6</servlet-name>
  	<url-pattern>/payData</url-pattern>
  </servlet-mapping>
  
  
  
  
  
  
</web-app>